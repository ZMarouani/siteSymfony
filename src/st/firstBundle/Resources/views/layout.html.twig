{%  extends '::layout.html.twig' %}

{%  block stylesheets %}
    {{ parent() }}
    <style>
        #pac-input {
            background-color: #fff;
            font-family: Roboto;
            font-size: 15px;
            font-weight: 300;
            margin-left: 12px;
            padding: 0 11px 0 13px;
            text-overflow: ellipsis;
            width: 300px;
        }

        #pac-input:focus {
            border-color: #4d90fe;
        }

        .pac-container {
            font-family: Roboto;
        }

        #type-selector {
            color: #fff;
            background-color: #4d90fe;
            padding: 5px 11px 0px 11px;
        }

        #type-selector label {
            font-family: Roboto;
            font-size: 25px;
            font-weight: 300;
        }
        #target {
            width: 345px;
        }
    </style>

{% endblock  %}

{% block body %}
    <input id="pac-input" class="controls" type="text" placeholder="Search Box">
    <div id="map" style="width:600px;height:600px;background:blue"> my nigga </div>

{%  endblock  %}

